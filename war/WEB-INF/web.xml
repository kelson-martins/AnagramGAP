<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Assignment1CC</servlet-name>
		<servlet-class>com.webapp.kelson.RootServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AnagramServlet</servlet-name>
		<servlet-class>com.webapp.kelson.AnagramServlet</servlet-class>
	</servlet>
		
	<servlet-mapping>
		<servlet-name>Assignment1CC</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AnagramServlet</servlet-name>
		<url-pattern>/AnagramServlet</url-pattern>
	</servlet-mapping>	
</web-app>
